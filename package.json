    "chalk": "5.4.1",
    "commander": "13.0.0",
    "jszip": "3.10.1",
    "ora": "8.1.1",
    "vite": "6.0.6",
    "jiti": "2.4.2",
    "vitest": "2.1.8",
    "zod": "3.24.1"
  },
  "devDependencies": {
    "@types/node": "22.10.2",
    "@typescript-eslint/eslint-plugin": "8.19.0",
    "@typescript-eslint/parser": "8.19.0",
    "eslint": "9.17.0",
    "prettier": "3.4.2",
    "tsup": "8.3.5",
    "typescript": "5.7.2"
  }
